{
    "name": "MyBrowser",
    "description": "Go from light mode to dark mode and vice versa",
    "version": "1.0.0",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "test.js"
        ],
    "persistent": true
    },
    "browser_action": {
        "default_title": "MyBrowser",
        "default_popup": "index.html"
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs"
    ]
}